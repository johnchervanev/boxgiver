- community_location_lat = @current_community.location ? @current_community.location.latitude : nil
- community_location_lon = @current_community.location ? @current_community.location.longitude : nil

#style-selector-control.map-control.gmnoprint{style: 'z-index: 0;position: absolute;left: 188px;top: 0px;margin: 10px 10px 10px -5px;border: none;'}
  %select#style-selector.selector-control{style: 'border: none;font-size: 17px;'}
    %option{value: 'default'} Default
    %option{value: 'night'} Night mode
- content_for :extra_javascript do
  :javascript
    initialize_labels();

    $(document).ready(function() {
      initialize_listing_map(
        #{raw listings_for_map(@listings).to_json},
        "#{community_location_lat}",
        "#{community_location_lon}",
        #{raw viewport.to_json},
        "#{I18n.locale}",
        "#{@current_community.use_community_location_as_default}");
      });

#map-canvas.map
